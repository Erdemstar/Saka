@model insecure_direct_object_reference_view_profile.Core.Entity.Account.AccountEntity
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="container" style="margin-top: 10%;">
    <div class="row">
        <div class="col-md-2"></div>

        <div class="col-md-8">
            
          <h1>Profile</h1>
          
          <form>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label>Email</label>
                <input type="email" class="form-control" value="@Model.Email">
              </div>
              <div class="form-group col-md-6">
                <label>Password</label>
                <input type="password" class="form-control" value="@Model.Password">
              </div>
            </div>

            <div class="form-row">
              <div class="form-group col-md-6">
                <label>Name</label>
                <input type="text" class="form-control" value="@Model.Name">
              </div>
              <div class="form-group col-md-6">
                <label>Lastname</label>
                <input type="text" class="form-control"  value="@Model.Lastname">
              </div>
            </div>

            <div class="form-group">
              <label>Address</label>
              <input type="text" class="form-control" value="@Model.Address">
            </div>
            <div class="form-group">
              <label>Hobby</label>
              <input type="text" class="form-control" value="@Model.Hobby">
            </div>
           
          </form>
            
        </div>

        <div class="col-md-2"></div>
    </div>

</div>