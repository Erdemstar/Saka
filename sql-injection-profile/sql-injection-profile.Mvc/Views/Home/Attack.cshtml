@model IEnumerable<sql_injection_profile.Core.Entity.Profile.ProfileEntity>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container" style="margin-top: 10%;">
    <div class="row">
        <div class="col-md-2"></div>

        <div class="col-md-8">

            <div>
                <h2>Registered User</h2>
                <ul>
                    <li>
                        <a asp-action="Attack" asp-controller="Home" asp-route-profileId="1">User 1</a>
                    </li>
                    <li>
                        <a asp-action="Attack" asp-controller="Home" asp-route-profileId="2">User 2</a>
                    </li>
                    <li>
                        <a asp-action="Attack" asp-controller="Home" asp-route-profileId="3">User 3</a>
                    </li>
                </ul>
            </div>

            <div style="margin-top: 5%">
                <table class="table">
                  <thead>
                    <tr>
                      <th scope="col">Name</th>
                      <th scope="col">Surname</th>
                      <th scope="col">Address</th>
                    </tr>
                  </thead>
                  <tbody>
                  @foreach (var profile in Model)
                  {
                      <tr>
                          <td>@profile.Name</td>
                          <td>@profile.Surname</td>
                          <td>@profile.Address</td>
                      </tr>
                  }
                  </tbody>
                </table>

            </div>


        </div>


        <div class="col-md-2"></div>
    </div>

</div>